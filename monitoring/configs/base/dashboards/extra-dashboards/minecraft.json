{
  "annotations": {
    "list": []
  },
  "title": "Minecraft Server Dashboard",
  "panels": [
    {
      "title": "Players Online",
      "type": "stat",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "minecraft_players_online"
        }
      ]
    },
    {
      "title": "Server Memory Usage",
      "type": "graph",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "process_resident_memory_bytes{namespace=\"minecraft\"}"
        }
      ]
    }
  ]
}
